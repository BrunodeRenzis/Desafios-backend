<div class=''>
    <h1 class="text-center text-bg-dark p-3">Nuestras Computadoras</h1>

    <table class="table table-light m-auto mt-5" style="width: 70%;" >
        <thead>
            <tr>
                <th scope="col"> Computadora </th>
                <th scope="col"> Precio </th>
                <th scope="col"> Imagen </th>
            </tr>
        </thead>
        <tbody>
            {{#each productos}}
                <tr>
                    <td> {{ this.title }} </td>
                    <td> {{ this.price }} </td>
                    <td><img src="{{this.thumbnail}}" width="100px" alt="{{ this.title }}"> </td>
                </tr>
            {{/each}}
        </tbody>
    </table>

    <form class="p-3" action="/" method="get">
        <button class="btn btn-dark">Volver</button>
    </form>

    <div class="text-center">
    <div>
        <p>Flor Hnatiuk</p>
        <p>2022</p>
    </div>
</div>
</div>